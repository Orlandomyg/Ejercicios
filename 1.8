#include <stdio.h>
#include <stdlib.h>

int main()
{
    int c,b,t,l;
    c=0;
    b=0;
    t=0;
    l=0;

    while((c=getchar()) != EOF)

    {
        if (c == ' ')
        {
            ++b;
        }
        if (c == '\t')
        {
            ++t;
        }
        if (c == '\n')
        {
            ++l;
        }



    }
    printf("Blancos: %d \t Tabulaciones: %d \t Lineas: %d\n",b,t,l);


    return 0;
}
